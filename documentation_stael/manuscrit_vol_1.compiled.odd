</elementSpec><elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" module="msdescription" xml:id="MSCONTENTS" ident="msContents">
  <gloss versionDate="2007-07-04" xml:lang="en">manuscript contents</gloss>
  <gloss versionDate="2007-12-20" xml:lang="ko">원고 내용</gloss>
