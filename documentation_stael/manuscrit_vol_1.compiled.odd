  </exemplum><exemplum versionDate="2008-04-06" xml:lang="fr">
    <egXML xmlns="http://www.tei-c.org/ns/Examples" source="#fr-ex-Lavrentev-TEI-BFM">
      <handNote xml:id="fr_scribe1">Premier scribe : <name type="person">inconnu</name> 
         </handNote>
      <handNote xml:id="fr_rubricateur1">Premier rubricateur : <name type="person">inconnu</name> 
         </handNote>
    </egXML>
  </exemplum><exemplum xml:lang="zh-TW">
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
